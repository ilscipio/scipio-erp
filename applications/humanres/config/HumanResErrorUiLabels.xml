<resource xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/ofbiz-properties.xsd">
    <property key="HumanResErrorPositionOrRequisitionMustBeSpecified">
        <value xml:lang="de">Eine Position oder Jobanforderung muss angegeben werden</value>
        <value xml:lang="en">One of position or job requisition must be specified</value>
    </property>
    <property key="HumanResErrorInvalidJobRequisition">
        <value xml:lang="de">Ungültige Job Anforderung angegeben</value>
        <value xml:lang="en">Invalid job requisition specified</value>
    </property>
    <property key="HumanResErrorInvalidEmplPosition">
        <value xml:lang="de">Ungültige Position angegeben</value>
        <value xml:lang="en">Invalid position specified</value>
    </property>
    <property key="HumanResErrorAppPositionMustEqualRequisitionPosition">
        <value xml:lang="de">Die Position auf dem Antrag ist die gleiche wie die Position auf dem Arbeitsanforderung sein</value>
        <value xml:lang="en">The position on the application must be the same as the position on the job requisition</value>
    </property>
</resource>
